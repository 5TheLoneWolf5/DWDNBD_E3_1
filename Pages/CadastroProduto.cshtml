@functions {
    string ObterParteDoDia (DateTime dt)
    {
        var parteDoDia = "manhã";

        if (DateTime.Now.Hour > 12 && DateTime.Now.Hour < 18)
            parteDoDia = "tarde";
        else if (DateTime.Now.Hour > 18)
            parteDoDia = "noite";

        return parteDoDia;
    }
    public class Cidade
    {
        public string Nome { get; set; }
        public string Pais { get; set; }
    }
    List<Cidade> cidades = new List<Cidade>
        {
            new Cidade { Nome = "Rio de Janeiro", Pais = "Brasil" },
            new Cidade { Nome = "Moscow", Pais = "Russia" },
            new Cidade { Nome = "Buenos Aires", Pais = "Argentina" },
            new Cidade { Nome = "Montevideu", Pais = "Uruguai" }
        };
}

