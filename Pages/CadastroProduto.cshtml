@if(DateTime.Now.Hour <= 12) {
    <h3>Bom dia</h3>
}
else if (DateTime.Now.Hour <= 18) {
    <h3>Boa tarde</h3>
}
else {
    <h3>Boa noite</h3>
}

<ul>
    @foreach (var cidade in cidades)
    {
        <li>@cidade.Nome - @cidade.Pais</li>
    }
</ul>

